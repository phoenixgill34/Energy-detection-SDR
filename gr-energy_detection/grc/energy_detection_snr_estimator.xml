<?xml version="1.0"?>
<block>
  <name>Energy Detector</name>
  <key>energy_detection_snr_estimator</key>
  <category>[energy_detection]</category>
  <import>import energy_detection</import>
  <make>energy_detection.snr_estimator(length=$length)</make>

  <param>
	<name>Length</name>
	<key>length</key>
	<type>int</type>
